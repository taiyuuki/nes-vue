{
  "name": "nes-vue",
  "version": "1.8.2",
  "description": "A NES(FC) emulator component for Vue 3",
  "module": "./dist/nes-vue.es.js",
  "main": "./dist/nes-vue.umd.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nes-vue.es.js",
      "require": "./dist/nes-vue.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "fc",
    "nes",
    "jsnes",
    "vue",
    "nes emulator",
    "fc emulator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/nes-vue.git"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "patch": "bump patch",
    "minor": "bump minor",
    "publish": "pnpm publish --registry https://registry.npmjs.org",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build-gitee": "set GITEE=1 && pnpx vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@taiyuuki/eslint-config": "^1.3.0",
    "@taiyuuki/utils": "^0.4.7",
    "@types/node": "^18.19.31",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^9.0.0",
    "jsnes": "^1.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^3.2.10",
    "vite-plugin-dts": "^3.8.3",
    "vitepress": "1.0.0-beta.1",
    "vitest": "^0.25.8",
    "vue": "^3.4.23",
    "vue-tsc": "^1.8.27"
  }
}
