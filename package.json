{
  "name": "nes-vue",
  "version": "1.9.0",
  "description": "A NES(FC) emulator component for Vue 3",
  "module": "./dist/nes-vue.es.js",
  "main": "./dist/nes-vue.umd.js",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nes-vue.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "fc",
    "nes",
    "jsnes",
    "vue",
    "nes emulator",
    "fc emulator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/nes-vue.git"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "patch": "bump patch",
    "minor": "bump minor",
    "publish": "pnpm publish --registry https://registry.npmjs.org",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build-gitee": "set GITEE=1 && pnpx vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nesjs/core": "^1.0.10"
  },
  "devDependencies": {
    "@taiyuuki/eslint-config": "^1.4.18",
    "@taiyuuki/utils": "^0.5.3",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^9.24.0",
    "nes-vue": "link:./dist/nes-vue.e.js",
    "rollup-plugin-delete": "^3.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-dts": "^4.5.3",
    "vitepress": "1.0.0-beta.1",
    "vitest": "^3.1.1",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}